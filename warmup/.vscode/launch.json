    "version": "0.2.0",
    "configurations": [
        {
            "name": "C debugger",
            "type": "cppdbg",
            "request": "launch",
            // "program": "/nfs/ug/homes-2/c/cheny776/ece344/vscode Executables/${fileBasenameNoExtension}",
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "args": ["cat and dogs"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            // "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "preLaunchTask": "C/C++: gcc 生成活动文件"
        }
    ]
}

